import java.util.*;
public class Main 
{
    public static void main(String[] args)
    {
        Scanner sc = new Scanner(System.in);
        String a= sc.nextLine();
        String[] r= a.split(" ");
        int l = r.length;
        
        for(int i= 0;i<l;i++)
        {
            String m=r[i];
            StringBuffer sb= new StringBuffer(m);
            System.out.print(sb.reverse() + " ");
        }  
    }
}
